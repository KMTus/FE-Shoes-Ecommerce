{
  "expo": {
    "name": "fe-shoe-ecommerce",
    "slug": "fe-shoe-ecommerce",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashImg.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", [
      "expo-font",
      {
        "fonts": [
          "./assets/fonts/OFL/Outfit-VariableFont_wght.ttf",
          "./assets/fonts/OFL/static/Outfit-Black.ttf",
          "./assets/fonts/OFL/static/Outfit-Bold.ttf",
          "./assets/fonts/OFL/static/Outfit-ExtraBold.ttf",
          "./assets/fonts/OFL/static/Outfit-ExtraLight.ttf",
          "./assets/fonts/OFL/static/Outfit-Light.ttf",
          "./assets/fonts/OFL/static/Outfit-Medium.ttf",
          "./assets/fonts/OFL/static/Outfit-Regular.ttf",
          "./assets/fonts/OFL/static/Outfit-SemiBold.ttf",
          "./assets/fonts/OFL/static/Outfit-Thin.ttf"
        ]
      }
    ], "expo-localization"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
